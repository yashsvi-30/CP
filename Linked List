  void reverse(Node* head){
    if(head==NULL) return;
    Node* prev;
    Node* curr = head;
    Node* next;
    while(curr!=NULL){
        next = curr->next;
        curr->next = prev;
        prev=curr;
        curr=next;
    }
  }
  
