𝐑𝐞𝐯𝐞𝐫𝐬𝐞 𝐚 𝐥𝐢𝐧𝐤𝐞𝐝 𝐥𝐢𝐬𝐭 :
  void reverse(Node* head){
    if(head==NULL) return;
    Node* prev;
    Node* curr = head;
    Node* next;
    while(curr!=NULL){
        next = curr->next;
        curr->next = prev;
        prev=curr;
        curr=next;
    }
  }
  
  𝐃𝐞𝐥𝐞𝐭𝐞 𝐧𝐨𝐝𝐞 𝐢𝐧 𝐃𝐨𝐮𝐛𝐥𝐲 𝐋𝐢𝐧𝐤𝐞𝐝 𝐋𝐢𝐬𝐭:
 void deleteNode(Node **head_ref, int x)
{
    struct Node *p=(*head_ref);
    while(x>1)
    {
        p=p->next;
        x--;
    }
    if(p->prev==NULL)
    {
        p=p->next;
        p->prev=NULL;
        *head_ref=p;
        //delete(p->prev);
    }
    else if(p->next==NULL)
    {
        p=p->prev;
        p->next=NULL;
    }
    else
    {
    p->prev->next=p->next;
    p->next->prev=p->prev;
    }
}
  
