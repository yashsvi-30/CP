ll countStrings(int n) {
	    ll a[n];
	    ll b[n];
	    a[0]=1;
	    b[0]=1;
	    for(int i=1;i<n;i++){
	        a[i]=a[i-1]+b[i-1];
	        b[i]=b[i-1];
	    }
	    return a[n-1]+b[n-1];
	}
  
ğ„ğğ¢ğ­ ğƒğ¢ğ¬ğ­ğšğ§ğœğ:
int editDistance(string a,string b,int n,int m){
    int dp[n+1][m+1];
    for(int i=0;i<n+1;i++){
    for(int j=0;j<m+1;j++){
      if(n==0){
        dp[i][j] = j;
      }
      else if(m==0){
        dp[i][j]=i;
      }
      else if(a[i-1]==b[j-1]){
        dp[i][j] = dp[i-1][j-1];
      }
      else{
        dp[i][j] = 1 + + min(dp[i][j - 1],dp[i - 1][j], dp[i - 1][j - 1]);
      }     
    }
    }
    return dp[n][m];
}

ğ‘ğ¨ğ ğ‚ğ®ğ­ğ­ğ¢ğ§ğ :
int cutRod(int price[], int n)
{
   int val[n+1];
   val[0] = 0;
   int i, j;
   for (i = 1; i<=n; i++)
   {
       int max_val = INT_MIN;
       for (j = 0; j < i; j++)
         max_val = max(max_val, price[j] + val[i-j-1]);
       val[i] = max_val;
   }
   return val[n];
}

ğ‹ğ¨ğ§ğ ğğ¬ğ­ ğˆğ§ğœğ«ğğšğ¬ğ¢ğ§ğ  ğ’ğ®ğ›ğ¬ğğªğ®ğğ§ğœğ:
int lis(int arr[], int n)
{
    int lis[n];
    lis[0] = 1;
    for (int i = 1; i < n; i++) {
        lis[i] = 1;
        for (int j = 0; j < i; j++)
            if (arr[i] > arr[j] && lis[i] < lis[j] + 1)
                lis[i] = lis[j] + 1;
    }
    return *max_element(lis, lis + n);
}

ğŒğšğ±ğ¢ğ¦ğ®ğ¦ ğ¬ğ®ğ¦ ğ¢ğ§ğœğ«ğğšğ¬ğ¢ğ§ğ  ğ¬ğ®ğ›ğ¬ğğªğ®ğğ§ğœğ:
int maxSumIS(int arr[], int n)  
	{ 
	    int dp[n];
	    dp[0]=arr[0];
	    for(int i=1;i<n;i++){
	        dp[i]=arr[i];
	        for(int j=0;j<i;j++){
	            if(arr[j]<arr[i] && dp[i]<dp[j]+arr[i]){
	                dp[i]=dp[j]+arr[i];
	            }
	        }
	    }
	    return *max_element(dp,dp+n);
	}
	
ğŒğšğ± ğ¥ğğ§ğ ğ­ğ¡ ğœğ¡ğšğ¢ğ§:
int maxChainLen(struct val p[],int n)
{
    int dp[n];
    int i,j=0;
    for ( i = 0; i < n; i++ )
        dp[i] = 1;
    for ( i = 1; i < n; i++ )
        for ( j = 0; j < i; j++ )
            if ( p[i].first > p[j].second &&
                    dp[i] < dp[j] + 1)
                dp[i] = dp[j] + 1;
    int max=0;
    for ( i = 0; i < n; i++ )
        if ( max < dp[i] )
            max = dp[i];
  return max;
}

